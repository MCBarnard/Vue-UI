{"ast":null,"code":"import NotificationComponent from \"@/components/Vue-UI/NotificationComponent\";\nimport ShowcaseComponent from \"@/components/ShowcaseComponent\";\nimport SelectComponent from \"@/components/Controls/SelectComponent\";\nimport PrismComponent from \"@/components/PrismComponent\";\nimport TableComponent from \"@/components/TableComponent\";\nimport * as AccordionComponentMarkdown from \"@/assets/ComponentMarkdown/AccordionComponentMarkdown\";\nexport default {\n  components: {\n    ShowcaseComponent,\n    PrismComponent,\n    TableComponent,\n    NotificationComponent,\n    SelectComponent\n  },\n  data() {\n    return {\n      key: 0,\n      variant: \"\",\n      deleted: false,\n      variantOptions: [\"info\", \"success\", \"warning\", \"danger\"],\n      code: AccordionComponentMarkdown.default,\n      propHeadings: [\"Prop\", \"Type\", \"Example\", \"Required\", \"Description\"],\n      propDetails: [[\"autoClose\", \"Boolean\", \"true\", \"No\", \"If true, the accordion will automatically close previous tabs\"], [\"rounded\", \"Boolean\", \"true\", \"No\", \"Will round the top and bottom items giving it a rounded look\"], [\"items\", \"Array\", \"[{title: 'Some question', content: 'some answer'}]\", \"Yes\", \"The content of your accordion\"]]\n    };\n  },\n  methods: {\n    variantChanged(item) {\n      return this.variant = item;\n    },\n    acknowledgeNotification() {\n      if (this.deleted) {\n        this.key++;\n        this.deleted = false;\n      } else {\n        this.$refs.notifications.hideNotification();\n      }\n    },\n    handleAcknowledged() {\n      this.deleted = true;\n    }\n  },\n  computed: {\n    computedVariant() {\n      return this.variant;\n    },\n    notificationButtonText() {\n      return this.deleted ? \"Reset\" : \"Acknowledge Notification\";\n    }\n  }\n};","map":{"version":3,"names":["NotificationComponent","ShowcaseComponent","SelectComponent","PrismComponent","TableComponent","AccordionComponentMarkdown","components","data","key","variant","deleted","variantOptions","code","default","propHeadings","propDetails","methods","variantChanged","item","acknowledgeNotification","$refs","notifications","hideNotification","handleAcknowledged","computed","computedVariant","notificationButtonText"],"sources":["/Users/thinusbarnard/Desktop/Personal Projects/vue-ui-library/src/views/Interface/NotificationPage.vue"],"sourcesContent":["<template>\n  <div class=\"notification-page\">\n    <ShowcaseComponent :key=\"key\">\n      <template v-slot:component>\n        <notification-component\n          :variant=\"computedVariant\"\n          :dark=\"false\"\n          @closed=\"handleAcknowledged\"\n          ref=\"notifications\"\n        ></notification-component>\n      </template>\n      <template v-slot:controls>\n        <table class=\"props-table\">\n          <tr>\n            <td>variant</td>\n            <td>\n              <select-component\n                :state=\"computedVariant\"\n                @selected=\"variantChanged\"\n                :data=\"variantOptions\"\n                defaults=\"default\"\n              />\n            </td>\n            <td class=\"divider\"></td>\n            <td>hideNotification()</td>\n            <td>\n              <button @click=\"acknowledgeNotification\">\n                {{ notificationButtonText }}\n              </button>\n            </td>\n          </tr>\n        </table>\n      </template>\n      <template v-slot:code>\n        <prism-component :code=\"code\" nameOfFile=\"AccordionComponent.vue\" />\n      </template>\n      <template v-slot:docs>\n        <table-component :headings=\"propHeadings\" :items=\"propDetails\" />\n      </template>\n    </ShowcaseComponent>\n  </div>\n</template>\n\n<script>\nimport NotificationComponent from \"@/components/Vue-UI/NotificationComponent\";\nimport ShowcaseComponent from \"@/components/ShowcaseComponent\";\nimport SelectComponent from \"@/components/Controls/SelectComponent\";\nimport PrismComponent from \"@/components/PrismComponent\";\nimport TableComponent from \"@/components/TableComponent\";\nimport * as AccordionComponentMarkdown from \"@/assets/ComponentMarkdown/AccordionComponentMarkdown\";\n\nexport default {\n  components: {\n    ShowcaseComponent,\n    PrismComponent,\n    TableComponent,\n    NotificationComponent,\n    SelectComponent,\n  },\n  data() {\n    return {\n      key: 0,\n      variant: \"\",\n      deleted: false,\n      variantOptions: [\"info\", \"success\", \"warning\", \"danger\"],\n      code: AccordionComponentMarkdown.default,\n      propHeadings: [\"Prop\", \"Type\", \"Example\", \"Required\", \"Description\"],\n      propDetails: [\n        [\n          \"autoClose\",\n          \"Boolean\",\n          \"true\",\n          \"No\",\n          \"If true, the accordion will automatically close previous tabs\",\n        ],\n        [\n          \"rounded\",\n          \"Boolean\",\n          \"true\",\n          \"No\",\n          \"Will round the top and bottom items giving it a rounded look\",\n        ],\n        [\n          \"items\",\n          \"Array\",\n          \"[{title: 'Some question', content: 'some answer'}]\",\n          \"Yes\",\n          \"The content of your accordion\",\n        ],\n      ],\n    };\n  },\n  methods: {\n    variantChanged(item) {\n      return (this.variant = item);\n    },\n    acknowledgeNotification() {\n      if (this.deleted) {\n        this.key++;\n        this.deleted = false;\n      } else {\n        this.$refs.notifications.hideNotification();\n      }\n    },\n    handleAcknowledged() {\n      this.deleted = true;\n    },\n  },\n  computed: {\n    computedVariant() {\n      return this.variant;\n    },\n    notificationButtonText() {\n      return this.deleted ? \"Reset\" : \"Acknowledge Notification\";\n    },\n  },\n};\n</script>\n\n<style scoped lang=\"scss\">\n* {\n  box-sizing: border-box;\n}\n\n.notification-page {\n  padding-top: 3rem;\n\n  .props-table {\n    td {\n      padding: 0 5px;\n    }\n    .divider {\n      width: 50px;\n      height: 100%;\n      position: relative;\n\n      &:after {\n        content: \"\";\n        position: absolute;\n        top: -2px;\n        transform: translateX(-50%);\n        left: 50%;\n        width: 2px;\n        height: calc(100% + 4px);\n        background: grey;\n      }\n    }\n  }\n}\n</style>\n"],"mappings":"AA4CA,OAAOA,qBAAoB,MAAO,2CAA2C;AAC7E,OAAOC,iBAAgB,MAAO,gCAAgC;AAC9D,OAAOC,eAAc,MAAO,uCAAuC;AACnE,OAAOC,cAAa,MAAO,6BAA6B;AACxD,OAAOC,cAAa,MAAO,6BAA6B;AACxD,OAAO,KAAKC,0BAAyB,MAAO,uDAAuD;AAEnG,eAAe;EACbC,UAAU,EAAE;IACVL,iBAAiB;IACjBE,cAAc;IACdC,cAAc;IACdJ,qBAAqB;IACrBE;EACF,CAAC;EACDK,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,GAAG,EAAE,CAAC;MACNC,OAAO,EAAE,EAAE;MACXC,OAAO,EAAE,KAAK;MACdC,cAAc,EAAE,CAAC,MAAM,EAAE,SAAS,EAAE,SAAS,EAAE,QAAQ,CAAC;MACxDC,IAAI,EAAEP,0BAA0B,CAACQ,OAAO;MACxCC,YAAY,EAAE,CAAC,MAAM,EAAE,MAAM,EAAE,SAAS,EAAE,UAAU,EAAE,aAAa,CAAC;MACpEC,WAAW,EAAE,CACX,CACE,WAAW,EACX,SAAS,EACT,MAAM,EACN,IAAI,EACJ,+DAA+D,CAChE,EACD,CACE,SAAS,EACT,SAAS,EACT,MAAM,EACN,IAAI,EACJ,8DAA8D,CAC/D,EACD,CACE,OAAO,EACP,OAAO,EACP,oDAAoD,EACpD,KAAK,EACL,+BAA+B,CAChC;IAEL,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,cAAcA,CAACC,IAAI,EAAE;MACnB,OAAQ,IAAI,CAACT,OAAM,GAAIS,IAAI;IAC7B,CAAC;IACDC,uBAAuBA,CAAA,EAAG;MACxB,IAAI,IAAI,CAACT,OAAO,EAAE;QAChB,IAAI,CAACF,GAAG,EAAE;QACV,IAAI,CAACE,OAAM,GAAI,KAAK;MACtB,OAAO;QACL,IAAI,CAACU,KAAK,CAACC,aAAa,CAACC,gBAAgB,CAAC,CAAC;MAC7C;IACF,CAAC;IACDC,kBAAkBA,CAAA,EAAG;MACnB,IAAI,CAACb,OAAM,GAAI,IAAI;IACrB;EACF,CAAC;EACDc,QAAQ,EAAE;IACRC,eAAeA,CAAA,EAAG;MAChB,OAAO,IAAI,CAAChB,OAAO;IACrB,CAAC;IACDiB,sBAAsBA,CAAA,EAAG;MACvB,OAAO,IAAI,CAAChB,OAAM,GAAI,OAAM,GAAI,0BAA0B;IAC5D;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}