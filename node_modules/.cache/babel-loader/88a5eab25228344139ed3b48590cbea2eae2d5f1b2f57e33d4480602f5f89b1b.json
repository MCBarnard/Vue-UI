{"ast":null,"code":"import ShowcaseComponent from \"@/components/ShowcaseComponent\";\nimport LinkComponent from \"@/components/Vue-UI/LinkComponent\";\nimport SelectComponent from \"@/components/Controls/SelectComponent\";\nimport InputComponent from \"@/components/Controls/InputComponent\";\nimport ToggleComponent from \"@/components/Controls/ToggleComponent\";\nimport PrismComponent from \"@/components/PrismComponent\";\n// import * as LinkComponentMarkdown from \"@/assets/ComponentMarkdown/LinkComponentMarkdown\";\n\nexport default {\n  components: {\n    ShowcaseComponent,\n    LinkComponent,\n    SelectComponent,\n    InputComponent,\n    ToggleComponent,\n    PrismComponent\n  },\n  data() {\n    return {\n      routeName: \"link\",\n      text: \"Some Link going some where\",\n      linkTitle: \"A link to somewhere\",\n      targetToggle: true,\n      linkVariant: \"\",\n      href: \"\",\n      variantOptions: [\"info\", \"success\", \"warning\", \"danger\"],\n      code: \"<template><div>Some code</div></template>\"\n    };\n  },\n  methods: {\n    variantChanged(item) {\n      this.linkVariant = item;\n    },\n    handleLinkChange(item) {\n      if (item.target.value) {\n        this.routeName = \"link\";\n        this.href = \"\";\n        alert(`Will make this link point to {name: ${item.target.value}}`);\n      }\n    },\n    handleHrefChange(item) {\n      if (item.target.value) {\n        this.routeName = \"\";\n        this.href = item.target.value;\n        alert(`Will make this link point to src=\"${item.target.value}\"`);\n      }\n    },\n    handleTextChange(item) {\n      if (item.target.value) this.text = item.target.value;\n    },\n    handleLinkTitleChange(item) {\n      if (item.target.value) this.linkTitle = item.target.value;\n    },\n    handleToggledChange() {\n      console.log(\"=======\");\n      console.log(this.targetToggle);\n      console.log(!this.targetToggle);\n      console.log(\"=======\");\n      this.targetToggle = !this.targetToggle;\n    }\n  },\n  computed: {\n    selectedVariant() {\n      return this.linkVariant;\n    },\n    computedHref() {\n      return this.href;\n    },\n    computedText() {\n      return this.text;\n    },\n    computedRouteName() {\n      return this.routeName;\n    },\n    computedLinkTitle() {\n      return this.linkTitle;\n    },\n    computedTargetToggle() {\n      return this.targetToggle;\n    }\n  }\n};","map":{"version":3,"names":["ShowcaseComponent","LinkComponent","SelectComponent","InputComponent","ToggleComponent","PrismComponent","components","data","routeName","text","linkTitle","targetToggle","linkVariant","href","variantOptions","code","methods","variantChanged","item","handleLinkChange","target","value","alert","handleHrefChange","handleTextChange","handleLinkTitleChange","handleToggledChange","console","log","computed","selectedVariant","computedHref","computedText","computedRouteName","computedLinkTitle","computedTargetToggle"],"sources":["/Users/thinusbarnard/Desktop/Personal Projects/vue-ui-library/src/views/Components/LinkPage.vue"],"sourcesContent":["<template>\n  <div class=\"link-page\">\n    <ShowcaseComponent>\n      <template v-slot:component>\n        <p>\n          If only we had\n          <link-component\n            :to=\"computedRouteName\"\n            :href=\"computedHref\"\n            :text=\"computedText\"\n            :title=\"computedLinkTitle\"\n            :openOnNewPage=\"targetToggle\"\n            :variant=\"selectedVariant\"\n          />, that would save us a lot of time\n        </p>\n      </template>\n      <template v-slot:controls>\n        <table class=\"props-table\">\n          <tr>\n            <td>variant</td>\n            <td>\n              <select-component\n                @selected=\"variantChanged\"\n                :data=\"variantOptions\"\n                defaults=\"default\"\n              />\n            </td>\n            <td class=\"divider\"></td>\n            <td>to</td>\n            <td>\n              <input-component\n                :value=\"computedRouteName\"\n                @change=\"handleLinkChange\"\n              />\n            </td>\n          </tr>\n          <tr>\n            <td>title</td>\n            <td>\n              <input-component\n                :value=\"computedLinkTitle\"\n                @change=\"handleLinkTitleChange\"\n              />\n            </td>\n            <td class=\"divider\"></td>\n            <td>text</td>\n            <td>\n              <input-component\n                :value=\"computedText\"\n                @change=\"handleTextChange\"\n              />\n            </td>\n          </tr>\n          <tr>\n            <td>href</td>\n            <td>\n              <input-component\n                :value=\"computedHref\"\n                @change=\"handleHrefChange\"\n              />\n            </td>\n            <td class=\"divider\"></td>\n            <td>openOnNewPage</td>\n            <td>\n              <toggle-component\n                size=\"small\"\n                variant=\"info\"\n                :state=\"computedTargetToggle\"\n                @toggle=\"handleToggledChange\"\n              />\n            </td>\n          </tr>\n        </table>\n      </template>\n      <template v-slot:code>\n        <prism-component :code=\"code\" />\n      </template>\n    </ShowcaseComponent>\n  </div>\n</template>\n\n<script>\nimport ShowcaseComponent from \"@/components/ShowcaseComponent\";\nimport LinkComponent from \"@/components/Vue-UI/LinkComponent\";\nimport SelectComponent from \"@/components/Controls/SelectComponent\";\nimport InputComponent from \"@/components/Controls/InputComponent\";\nimport ToggleComponent from \"@/components/Controls/ToggleComponent\";\nimport PrismComponent from \"@/components/PrismComponent\";\n// import * as LinkComponentMarkdown from \"@/assets/ComponentMarkdown/LinkComponentMarkdown\";\n\nexport default {\n  components: {\n    ShowcaseComponent,\n    LinkComponent,\n    SelectComponent,\n    InputComponent,\n    ToggleComponent,\n    PrismComponent,\n  },\n  data() {\n    return {\n      routeName: \"link\",\n      text: \"Some Link going some where\",\n      linkTitle: \"A link to somewhere\",\n      targetToggle: true,\n      linkVariant: \"\",\n      href: \"\",\n      variantOptions: [\"info\", \"success\", \"warning\", \"danger\"],\n      code: \"<template><div>Some code</div></template>\",\n    };\n  },\n  methods: {\n    variantChanged(item) {\n      this.linkVariant = item;\n    },\n    handleLinkChange(item) {\n      if (item.target.value) {\n        this.routeName = \"link\";\n        this.href = \"\";\n        alert(`Will make this link point to {name: ${item.target.value}}`);\n      }\n    },\n    handleHrefChange(item) {\n      if (item.target.value) {\n        this.routeName = \"\";\n        this.href = item.target.value;\n        alert(`Will make this link point to src=\"${item.target.value}\"`);\n      }\n    },\n    handleTextChange(item) {\n      if (item.target.value) this.text = item.target.value;\n    },\n    handleLinkTitleChange(item) {\n      if (item.target.value) this.linkTitle = item.target.value;\n    },\n    handleToggledChange() {\n      console.log(\"=======\");\n      console.log(this.targetToggle);\n      console.log(!this.targetToggle);\n      console.log(\"=======\");\n      this.targetToggle = !this.targetToggle;\n    },\n  },\n  computed: {\n    selectedVariant() {\n      return this.linkVariant;\n    },\n    computedHref() {\n      return this.href;\n    },\n    computedText() {\n      return this.text;\n    },\n    computedRouteName() {\n      return this.routeName;\n    },\n    computedLinkTitle() {\n      return this.linkTitle;\n    },\n    computedTargetToggle() {\n      return this.targetToggle;\n    },\n  },\n};\n</script>\n\n<style scoped lang=\"scss\">\n* {\n  box-sizing: border-box;\n}\n\n.link-page {\n  padding-top: 3rem;\n\n  .props-table {\n    td {\n      padding: 0 5px;\n    }\n    .divider {\n      width: 50px;\n      height: 100%;\n      position: relative;\n\n      &:after {\n        content: \"\";\n        position: absolute;\n        top: -2px;\n        transform: translateX(-50%);\n        left: 50%;\n        width: 2px;\n        height: calc(100% + 4px);\n        background: grey;\n      }\n    }\n  }\n}\n</style>\n"],"mappings":"AAkFA,OAAOA,iBAAgB,MAAO,gCAAgC;AAC9D,OAAOC,aAAY,MAAO,mCAAmC;AAC7D,OAAOC,eAAc,MAAO,uCAAuC;AACnE,OAAOC,cAAa,MAAO,sCAAsC;AACjE,OAAOC,eAAc,MAAO,uCAAuC;AACnE,OAAOC,cAAa,MAAO,6BAA6B;AACxD;;AAEA,eAAe;EACbC,UAAU,EAAE;IACVN,iBAAiB;IACjBC,aAAa;IACbC,eAAe;IACfC,cAAc;IACdC,eAAe;IACfC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,MAAM;MACjBC,IAAI,EAAE,4BAA4B;MAClCC,SAAS,EAAE,qBAAqB;MAChCC,YAAY,EAAE,IAAI;MAClBC,WAAW,EAAE,EAAE;MACfC,IAAI,EAAE,EAAE;MACRC,cAAc,EAAE,CAAC,MAAM,EAAE,SAAS,EAAE,SAAS,EAAE,QAAQ,CAAC;MACxDC,IAAI,EAAE;IACR,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,cAAcA,CAACC,IAAI,EAAE;MACnB,IAAI,CAACN,WAAU,GAAIM,IAAI;IACzB,CAAC;IACDC,gBAAgBA,CAACD,IAAI,EAAE;MACrB,IAAIA,IAAI,CAACE,MAAM,CAACC,KAAK,EAAE;QACrB,IAAI,CAACb,SAAQ,GAAI,MAAM;QACvB,IAAI,CAACK,IAAG,GAAI,EAAE;QACdS,KAAK,CAAE,uCAAsCJ,IAAI,CAACE,MAAM,CAACC,KAAM,GAAE,CAAC;MACpE;IACF,CAAC;IACDE,gBAAgBA,CAACL,IAAI,EAAE;MACrB,IAAIA,IAAI,CAACE,MAAM,CAACC,KAAK,EAAE;QACrB,IAAI,CAACb,SAAQ,GAAI,EAAE;QACnB,IAAI,CAACK,IAAG,GAAIK,IAAI,CAACE,MAAM,CAACC,KAAK;QAC7BC,KAAK,CAAE,qCAAoCJ,IAAI,CAACE,MAAM,CAACC,KAAM,GAAE,CAAC;MAClE;IACF,CAAC;IACDG,gBAAgBA,CAACN,IAAI,EAAE;MACrB,IAAIA,IAAI,CAACE,MAAM,CAACC,KAAK,EAAE,IAAI,CAACZ,IAAG,GAAIS,IAAI,CAACE,MAAM,CAACC,KAAK;IACtD,CAAC;IACDI,qBAAqBA,CAACP,IAAI,EAAE;MAC1B,IAAIA,IAAI,CAACE,MAAM,CAACC,KAAK,EAAE,IAAI,CAACX,SAAQ,GAAIQ,IAAI,CAACE,MAAM,CAACC,KAAK;IAC3D,CAAC;IACDK,mBAAmBA,CAAA,EAAG;MACpBC,OAAO,CAACC,GAAG,CAAC,SAAS,CAAC;MACtBD,OAAO,CAACC,GAAG,CAAC,IAAI,CAACjB,YAAY,CAAC;MAC9BgB,OAAO,CAACC,GAAG,CAAC,CAAC,IAAI,CAACjB,YAAY,CAAC;MAC/BgB,OAAO,CAACC,GAAG,CAAC,SAAS,CAAC;MACtB,IAAI,CAACjB,YAAW,GAAI,CAAC,IAAI,CAACA,YAAY;IACxC;E