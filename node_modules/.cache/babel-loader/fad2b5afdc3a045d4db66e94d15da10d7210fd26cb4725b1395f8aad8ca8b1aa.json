: 100%;\n    height: 100%;\n    transition: all 0.8s ease;\n    z-index: 2;\n  }\n\n  &__left-shadow {\n    left: calc(-100% - 65px);\n    background-color: #fff;\n    box-shadow: 20px 0 20px 20px #fff;\n\n    &.done {\n      left: -50%;\n    }\n  }\n\n  &__right-shadow {\n    right: calc(-100% - 65px);\n    background-color: #fff;\n    box-shadow: -20px 0 20px 20px #fff;\n\n    &.done {\n      right: -50%;\n    }\n  }\n\n  &__pages {\n    width: 100%;\n    background-color: #03cd03;\n    height: 100%;\n  }\n\n  &__done {\n    position: absolute;\n    top: 50%;\n    left: 50%;\n    transform: translate(-50%, -50%);\n    z-index: 3;\n\n    &__icon {\n      width: 150px;\n      height: 150px;\n      border-radius: 50%;\n      border: 1px solid #000;\n      background-color: #efefef;\n      opacity: 0;\n      pointer-events: none;\n      transition: opacity 1.5s ease;\n      display: flex;\n      justify-content: center;\n      flex-direction: column;\n      text-align: center;\n      margin: auto;\n\n      &.done {\n        animation: fadeIn 1.5s ease forwards;\n      }\n    }\n    &__signed-up {\n      border-radius: 50%;\n      z-index: 3;\n      opacity: 0;\n      transition: opacity 2.5s ease;\n      text-align: center;\n\n      .heading {\n        font-size: 22px;\n        font-family: \"Lobster\", cursive;\n        color: #03cd03;\n        margin: 22px 0 0;\n      }\n\n      .subtitle {\n        font-size: 12px;\n        color: rgb(163, 163, 163);\n        margin: 0;\n      }\n\n      &.done {\n        animation: fadeIn 2.5s ease forwards;\n      }\n    }\n  }\n\n  @keyframes fadeIn {\n    0% {\n      opacity: 0;\n    }\n    50% {\n      opacity: 0;\n    }\n    to {\n      opacity: 1;\n    }\n  }\n}\n</style>\n"],"mappings":"AA0BA,OAAOA,gBAAe,MAAO,4BAA4B;AAEzD,eAAe;EACbC,UAAU,EAAE;IACVD,gBAAgB,CAAE;EACpB,CAAC;;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,KAAK;MAAE;MACbC,KAAK,EAAE,CACL;QACEC,IAAI,EAAE,YAAY;QAClBC,IAAI,EAAE,4BAA4B;QAClCC,SAAS,EAAE;MACb,CAAC,EAC