{"ast":null,"code":"import StepperComponent from \"../Vue-UI/StepperComponent\";\nimport InputComponent from \"../Vue-UI/InputComponent.vue\";\nexport default {\n  components: {\n    StepperComponent,\n    // Will watch the Steps data prop for the state in which it will behave\n    InputComponent\n  },\n  data() {\n    return {\n      done: false,\n      // Triggers the close screen\n      stepOne: {\n        username: \"\",\n        password: \"\",\n        accountType: \"\"\n      }\n    };\n  },\n  mounted() {\n    // setTimeout(() => {\n    //   this.done = true;\n    // }, 2000);\n  },\n  computed: {\n    stepOneDone() {\n      return this.stepOne.username !== \"\" && this.stepOne.password !== \"\" && this.stepOne.accountType !== \"\";\n    },\n    showStepper() {\n      return this.steps[0].completed || this.steps[this.steps.length - 1].completed;\n    },\n    computedSteps() {\n      return [{\n        icon: \"storefront\",\n        name: \"Credentials & Account Type\",\n        completed: this.stepOneDone\n      }, {\n        icon: \"share_location\",\n        name: \"Address & Business Details\",\n        completed: false\n      }, {\n        icon: \"account_balance\",\n        name: \"Banking & Payout Details\",\n        completed: false\n      }, {\n        icon: \"badge\",\n        name: \"Personal Account Details\",\n        completed: false\n      }, {\n        icon: \"checklist_rtl\",\n        name: \"Review & Comfirmation\",\n        completed: false\n      }];\n    }\n  }\n};","map":{"version":3,"names":["StepperComponent","InputComponent","components","data","done","stepOne","username","password","accountType","mounted","computed","stepOneDone","showStepper","steps","completed","length","computedSteps","icon","name"],"sources":["/Users/thinusbarnard/Desktop/Personal Projects/vue-ui-library/src/components/FullyBuilt/SignUp.vue"],"sourcesContent":["<template>\n  <div class=\"sign-up-component\">\n    <div :class=\"['sign-up-component__left-shadow', { done }]\"></div>\n    <div :class=\"['sign-up-component__right-shadow', { done }]\"></div>\n    <div class=\"sign-up-component__done\">\n      <div :class=\"['sign-up-component__done__icon', { done }]\">logo</div>\n      <div :class=\"['sign-up-component__done__signed-up', { done }]\">\n        <p class=\"heading\">Thanks you for signing up!</p>\n        <p class=\"subtitle\">\n          Be sure to check your email for the activation link!\n        </p>\n      </div>\n    </div>\n    <div class=\"sign-up-component__progress\" v-if=\"showStepper\">\n      <stepper-component\n        @manual-go-to=\"(item) => console.log(item.name)\"\n        :steps=\"computedSteps\"\n      />\n    </div>\n    <div class=\"sign-up-component__pages\">\n      <div class=\"sign-up-component__pages__one\">\n        <div class=\"sign-up-component__pages__one__logo\">logo</div>\n        <div class=\"sign-up-component__pages__one__form\">\n          <div class=\"sign-up-component__pages__one__form__username\">\n            <input-component variant=\"info\" label=\"Email\" full />\n          </div>\n          <div class=\"sign-up-component__pages__one__form__password\">logo</div>\n          <div class=\"sign-up-component__pages__one__form__account-type\">\n            logo\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport StepperComponent from \"../Vue-UI/StepperComponent\";\nimport InputComponent from \"../Vue-UI/InputComponent.vue\";\n\nexport default {\n  components: {\n    StepperComponent, // Will watch the Steps data prop for the state in which it will behave\n    InputComponent,\n  },\n  data() {\n    return {\n      done: false, // Triggers the close screen\n      stepOne: {\n        username: \"\",\n        password: \"\",\n        accountType: \"\",\n      },\n    };\n  },\n  mounted() {\n    // setTimeout(() => {\n    //   this.done = true;\n    // }, 2000);\n  },\n  computed: {\n    stepOneDone() {\n      return (\n        this.stepOne.username !== \"\" &&\n        this.stepOne.password !== \"\" &&\n        this.stepOne.accountType !== \"\"\n      );\n    },\n    showStepper() {\n      return (\n        this.steps[0].completed || this.steps[this.steps.length - 1].completed\n      );\n    },\n    computedSteps() {\n      return [\n        {\n          icon: \"storefront\",\n          name: \"Credentials & Account Type\",\n          completed: this.stepOneDone,\n        },\n        {\n          icon: \"share_location\",\n          name: \"Address & Business Details\",\n          completed: false,\n        },\n        {\n          icon: \"account_balance\",\n          name: \"Banking & Payout Details\",\n          completed: false,\n        },\n        {\n          icon: \"badge\",\n          name: \"Personal Account Details\",\n          completed: false,\n        },\n        {\n          icon: \"checklist_rtl\",\n          name: \"Review & Comfirmation\",\n          completed: false,\n        },\n      ]\n    }\n  },\n};\n</script>\n\n<style scoped lang=\"scss\">\n.sign-up-component {\n  width: 100%;\n  max-width: 600px;\n  background-color: #fff;\n  margin: 60px auto;\n  box-shadow: 0 0 20px -20px black;\n  border-radius: 20px;\n  position: relative;\n  overflow-x: hidden;\n  padding: 40px;\n\n  &__left-shadow,\n  &__right-shadow {\n    position: absolute;\n    top: 0;\n    width: 100%;\n    height: 100%;\n    transition: all 0.8s ease;\n    z-index: 2;\n  }\n\n  &__left-shadow {\n    left: calc(-100% - 65px);\n    background-color: #fff;\n    box-shadow: 20px 0 20px 20px #fff;\n\n    &.done {\n      left: -50%;\n    }\n  }\n\n  &__right-shadow {\n    right: calc(-100% - 65px);\n    background-color: #fff;\n    box-shadow: -20px 0 20px 20px #fff;\n\n    &.done {\n      right: -50%;\n    }\n  }\n\n  &__pages {\n    display: flex;\n    justify-content: center;\n    flex-direction: column;\n\n    &__one {\n      width: 100%;\n\n      &__logo {\n        width: 150px;\n        height: 150px;\n        border-radius: 50%;\n        border: 1px solid #000;\n        background-color: #efefef;\n        pointer-events: none;\n        display: flex;\n        justify-content: center;\n        flex-direction: column;\n        text-align: center;\n        margin: auto;\n      }\n\n      &__form {\n        max-width: 320px;\n        margin: auto;\n      }\n    }\n  }\n\n  &__done {\n    position: absolute;\n    top: 50%;\n    left: 50%;\n    transform: translate(-50%, -50%);\n    z-index: 3;\n    pointer-events: none;\n\n    &__icon {\n      width: 150px;\n      height: 150px;\n      border-radius: 50%;\n      border: 1px solid #000;\n      background-color: #efefef;\n      opacity: 0;\n      pointer-events: none;\n      transition: opacity 1.5s ease;\n      display: flex;\n      justify-content: center;\n      flex-direction: column;\n      text-align: center;\n      margin: auto;\n\n      &.done {\n        animation: fadeIn 1.5s ease forwards;\n      }\n    }\n    &__signed-up {\n      border-radius: 50%;\n      z-index: 3;\n      opacity: 0;\n      transition: opacity 2.5s ease;\n      text-align: center;\n\n      .heading {\n        font-size: 22px;\n        font-family: \"Lobster\", cursive;\n        color: #03cd03;\n        margin: 22px 0 0;\n      }\n\n      .subtitle {\n        font-size: 12px;\n        color: rgb(163, 163, 163);\n        margin: 0;\n      }\n\n      &.done {\n        animation: fadeIn 2.5s ease forwards;\n      }\n    }\n  }\n\n  @keyframes fadeIn {\n    0% {\n      opacity: 0;\n    }\n    50% {\n      opacity: 0;\n    }\n    to {\n      opacity: 1;\n    }\n  }\n}\n</style>\n"],"mappings":"AAqCA,OAAOA,gBAAe,MAAO,4BAA4B;AACzD,OAAOC,cAAa,MAAO,8BAA8B;AAEzD,eAAe;EACbC,UAAU,EAAE;IACVF,gBAAgB;IAAE;IAClBC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,KAAK;MAAE;MACbC,OAAO,EAAE;QACPC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,WAAW,EAAE;MACf;IACF,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR;IACA;IACA;EAAA,CACD;EACDC,QAAQ,EAAE;IACRC,WAAWA,CAAA